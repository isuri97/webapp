<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
>
	<!-- This is only here because Maven requires it to make a war. -->

	<servlet>
		<servlet-name>GetClaimServlet</servlet-name>
		<servlet-class>org.servlet.GetClaimServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>GetClaimServlet</servlet-name>
		<url-pattern>/claims</url-pattern>
	</servlet-mapping>

	<display-name>login</display-name>

	<servlet>
		<description></description>
		<display-name>LoginController</display-name>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>controllers.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/LoginController</url-pattern>
	</servlet-mapping>

</web-app>

